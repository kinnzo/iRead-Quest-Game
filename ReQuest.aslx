  <command>
    <scope>Shop</scope>
    <pattern>buy #object#</pattern>
    <unresolved>We don't sell what you are looking for.</unresolved>
    <script><![CDATA[
      if (object.num > 0) {
        if (player.money  >= object.price) {
          player.money = player.money-object.price
          object.num = object.num-1
          OutputTextNoBr ("You have bought a beautiful "+object.name+" for yourself.")
          CloneObject (object)
          AddToInventory (object)
          if (StartsWith(LCase(object.name),"hea")) {
            player.hearts = player.hearts+1
            msg ("You can see +1 Heart in your Status pane.")
          }
          firsttime {
            player.exp = player.exp+4
          }
          otherwise {
            player.exp = player.exp+2
          }
        }
        else {
          msg ("You can't afford it.")
        }
      }
      else {
        msg ("This object is out of stock.")
      }
      player.exp = player.exp+1
    ]]></script>
  </command>
