          player.urooms = player.urooms + 1
          firsttime {
            player.money = player.money+2
          }
        }
        else {
          msg ("Good try but you have to use it in some other way.")
        }
      }
      else {
        msg ("You do not have it.")
      }
    </script>
  </command>
  <command>
    <pattern>climb using #object1# to #object2#</pattern>
    <scope>canyon</scope>
    <script>
      if (Got(rope)) {
        if (StartsWith(LCase(object2.name),"magic ston")) {
          msg ("Congratulations! You completed your fifth task. Exit unlocked!")
          RemoveObject (rope)
          MakeExitVisible (to the castle gates)
          UnlockExit (to the castle gates)
          player.urooms = player.urooms + 1
          firsttime {
            player.money = player.money+3
          }
        }
        else {
          msg ("Good try but you have to use it in some other way.")
        }
      }
      else {
        msg ("You do not have it.")
      }
    </script>
  </command>
  <function name="update">
    player.exp = player.exp+1
  </function>
  <javascript src="interact.js" />
  <timer name="quest">
    <interval>30</interval>
    <enabled type="boolean">false</enabled>
    <script>
      firsttime {
        msg ("30 seconds have passed. I hope that you find the objects soon.")
      }
      otherwise {
        player.ach = player.ach+0.5
        msg ("Another 30 seconds have passed. Every second counts.")
      }
      JS.interact (player.alias, player.ach, player.exp, player.urooms, player.hearts, player.money)
    </script>
  </timer>
  <timer name="transfer">
    <interval>25</interval>
    <enabled />
    <script>
      JS.interact (player.alias, player.ach, player.exp, player.urooms, player.hearts, player.money)
    </script>
  </timer>
  <timer name="quest2">
    <interval>30</interval>
    <script>
      player.exp = player.exp+1
    </script>
  </timer>
</asl>