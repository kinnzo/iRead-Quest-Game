    <start type="script">
      play sound ("My Little Adventure1.mp3", false, true)
      player.hearts = 0
      options = NewStringList()
      player.money = 5
      list add (options, "yes")
      list add (options, "no")
      ShowMenu ("Would you like to see the leaderboard?", options, false) {
        switch (result) {
          case ("yes") {
            msg ("Great! Leaderboard will be displayed at the end of the game.")
            player.ach = player.ach+0.75
          }
          case ("no") {
            msg ("Ok. Let us focus on the world.")
          }
        }
        options1 = NewStringList()
        list add (options1, "yes")
        list add (options1, "no")
        ShowMenu ("Would you like to see the timer?", options1, false) {
          switch (result) {
            case ("yes") {
              msg ("Go ahead. The timer will start ticking.")
              player.ach = player.ach+1
              player.tflag = 1
            }
            case ("no") {
              msg ("Ok. No timer to disturb you.")
            }
          }
          msg ("What is your first name?")
          get input {
            player.alias = result
            JS.interact (player.alias, player.ach, player.exp, player.urooms, player.hearts, player.money)
            msg ("Thank You. Now type \"go to start\" to begin the game. You can type \"solve\" for tips. If you use \"solve\" for two times in the same room, the solution is displayed.")
            picture ("start.gif")
          }
        }
      }
    </start>
    <inituserinterface type="script">
      JS.setCommands ("Look;Help Me;Solve")
      JS.JS.eval ("$('#gameBorder').css('width', 'device-width, initial-scale=1');")
    </inituserinterface>
  </game>
